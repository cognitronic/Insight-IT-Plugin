<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
assembly="Insight.Accounts"
namespace="Insight.Accounts.Core.Domain">
    <class name="AccountAttachment" table="AccountAttachment">
        <id name="ID" column="ID">
            <generator class="identity"></generator>
        </id>
        <property name="AccountID" />
        <property name="FileName" />
        <property name="FullPath"  />
        <property name="Title" />
        <property name="EnteredBy" />
        <property name="ChangedBy" />
        <property name="Description" />
        <property name="DateCreated" />
        <property name="LastUpdated" />
        <property name="MarkedForDeletion" />
        <many-to-one name="AttachmentAccount" class="Insight.Accounts.Core.Domain.Account, Insight.Accounts" column="AccountID" insert="false" update="false" cascade="none" />
    </class>
</hibernate-mapping>